{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["webpack*", "main.js"],
    "rules": {
        "semi": ["error", "never"],
        "no-console": "off",
        "import/extensions": "off",
        "import/no-extraneous-dependencies": "off",
        "max-len": "off"
    }
}
